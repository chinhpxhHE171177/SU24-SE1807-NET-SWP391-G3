<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>SubjectListServlet</servlet-name>
        <servlet-class>controller.admin.SubjectListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateSubjectServlet</servlet-name>
        <servlet-class>controller.admin.UpdateSubjectServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddSubjectServlet</servlet-name>
        <servlet-class>controller.admin.AddSubjectServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteSubjectServlet</servlet-name>
        <servlet-class>controller.admin.DeleteSubjectServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubjectDetailServlet</servlet-name>
        <servlet-class>controller.admin.DetailSubjectServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateRegistrations</servlet-name>
        <servlet-class>controller.admin.UpdateRegistrations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>controller.admin.RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteRegistration</servlet-name>
        <servlet-class>controller.admin.DeleteRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddRegistration</servlet-name>
        <servlet-class>controller.admin.AddRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrationServlet</servlet-name>
        <servlet-class>controller.Registrations.registrationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubjectListServlet</servlet-name>
        <url-pattern>/admin/subject-list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateSubjectServlet</servlet-name>
        <url-pattern>/admin/update-subject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddSubjectServlet</servlet-name>
        <url-pattern>/admin/new-subject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteSubjectServlet</servlet-name>
        <url-pattern>/admin/delete-subject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubjectDetailServlet</servlet-name>
        <url-pattern>/admin/detail-subject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateRegistrations</servlet-name>
        <url-pattern>/registration/UpdateRegis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/registration/Regis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteRegistration</servlet-name>
        <url-pattern>/registration/DelRegis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddRegistration</servlet-name>
        <url-pattern>/registration/AddRegis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrationServlet</servlet-name>
        <url-pattern>/registration/list-regis</url-pattern>
    </servlet-mapping>
</web-app>
